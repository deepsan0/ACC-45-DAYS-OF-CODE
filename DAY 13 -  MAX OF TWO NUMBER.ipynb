    "def max_of_two_num(n,m):\n",
    "    if n>m:\n",
    "        return n\n",
    "    else:\n",
    "        return m   \n",
    "\n",
    "a=int(input(\"Enter first number:\"))\n",
    "b=int(input(\"Enter second number:\"))\n",
    "\n",
    "x=max_of_two_num(a,b)\n",
    "\n",
    "print(\"Maximum of\",a,\"and\",b,\"is\",x)"
   ]
  }
