   "source": [
    "#Q4. pythom program to remove punctuation from string\n",
    "\n",
    "punctuation = '''''!()-[]{};:'\"\\,<>./?@#$%^&*_~'''  \n",
    "my_str = input(\"Enter a string: \")  \n",
    "no_punct = \"\"  \n",
    "for char in my_str:  \n",
    "     if char not in punctuation:  \n",
    "        no_punct = no_punct + char  \n",
    "\n",
    "print(no_punct)"
   ]
