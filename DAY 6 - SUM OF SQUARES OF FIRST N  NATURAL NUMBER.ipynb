   "source": [
    "def SumofSquares(n):\n",
    "    s=0\n",
    "    for i in range(n+1):\n",
    "        s+=i**2\n",
    "    return s\n",
    "\n",
    "#input\n",
    "n=int(input(\"enter n: \"))\n",
    "print(\"sum of squares of first {} natural numbers: \".format(n),SumofSquares(n))\n"
   ]
